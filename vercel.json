{
  "name": "client-flow-360-crm",
  "version": 2,
  "functions": {
    "api/clients.js": {
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/api/clients/([^/]+)",
      "dest": "/api/clients.js"
    },
    {
      "src": "/api/clients",
      "dest": "/api/clients.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/landing.html",
      "dest": "/landing.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}